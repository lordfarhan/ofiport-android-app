<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="org.ofiport">

  <application
    android:name=".App"
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.Ofiport">
    <activity android:name=".activity.CompleteProfileActivity"></activity>
    <activity android:name=".activity.ExpenseActivity" />
    <activity android:name=".activity.IncomeActivity" />
    <activity android:name=".activity.MainActivity" />
    <activity android:name=".ui.SplashScreenActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name=".ui.landing.LandingActivity" />
    <activity android:name=".ui.overview.OverviewActivity" />
    <activity android:name=".ui.statistic.StatisticActivity" />
    <activity android:name=".ui.transaction.TransactionActivity" />
    <activity android:name=".ui.profile.ProfileActivity" />
    <activity android:name=".ui.profile.ProfileEditActivity" />
  </application>

</manifest>